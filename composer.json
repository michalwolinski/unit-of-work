{
  "name": "michalwolinski/unit-of-work",
  "description": "Unit of Work (Entity Manager) design pattern Laravel implementation.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michal Wolinski",
      "email": "firma@haxmedia.pl"
    }
  ],
  "minimum-stability": "dev",
  "keywords": [
    "eloquent",
    "laravel",
    "unit of work",
    "entity manager"
  ],
  "require": {
    "php": "^7.4",
    "laravel/framework": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0@dev",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "MichalWolinski\\UnitOfWork\\": "src/",
      "MichalWolinski\\UnitOfWork\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MichalWolinski\\UnitOfWork\\Providers\\UnitOfWorkServiceProvider"
      ]
    }
  }
}
